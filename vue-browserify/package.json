{
    "name": "vue-browserify",
    "description": "Example using Browserify with vueify",
    "author": "Sohel Amin",
    "private": true,
    "license": "MIT",
    "scripts": {
        "dev": "watchify -vd -p browserify-hmr -e src/main.js -o dist/build.js & http-server -o -s -c 1 -a localhost -p 8081",
        "build": "cross-env NODE_ENV=production browserify -g envify src/main.js | uglifyjs -c warnings=false -m > dist/build.js"
    },
    "dependencies": {
        "vue": "^2.0.1"
    },
    "devDependencies": {
        "babel-core": "^6.16.0",
        "babel-preset-es2015": "^6.16.0",
        "cross-env": "^3.1.1",
        "babelify": "^7.3.0",
        "browserify": "^13.1.0",
        "browserify-hmr": "^0.3.5",
        "envify": "^3.4.1",
        "http-server": "^0.9.0",
        "uglify-js": "^2.7.3",
        "vueify": "^9.1.0",
        "watchify": "^3.7.0"
    },
    "browserify": {
        "transform": [
        "vueify",
            "babelify"
        ]
    }
}